{% extends "html/base.html.j2" %}

{% block title %}
    {{ search_type }} search error(s)
{% endblock %}

{% block content %}
    <div class="content-bg">
        {% if errors|length > 0 %}
        <ul>
            {% for error in errors %}
            <li>Error: {{ error }}</li>
            {% endfor %}
        </ul>
        <hr/>
        {% endif %}
        <pre>{{ xml }}</pre>
    </div>
{% endblock %}

{% block dependenciesScripts %}
{% endblock %}